{
  "name": "generic-window-manager-electron",
  "version": "0.0.1",
  "description": "Generic window manager with tabs electron",
  "main": "./dist/main.bundle.js",
  "scripts": {
    "start": "electron --inspect=5858 ./dist/main.bundle.js",
    "dev": "rimraf dist && cross-env NODE_ENV=development webpack --watch --progress --colors",
    "prod": "rimraf dist && cross-env NODE_ENV=production webpack --progress --colors",
    "test": "mocha -r ts-node/register -r tsconfig-paths/register \"test/**/*.ts\"",
    "build:win": "electron-builder build --win --x64",
    "build:mac": "electron-builder build --mac --x64"
  },
  "build": {
    "appId": "com.devtography.electron_boilerplate",
    "productName": "Electron+React+TypeScript Boilerplate",
    "directories": {
      "app": "./dist/",
      "output": "./out/"
    },
    "mac": {
      "target": "pkg"
    },
    "win": {
      "target": "nsis",
      "asar": false
    },
    "buildVersion": "1.0.0"
  },
  "author": "Daniel Farina",
  "license": "MIT",
  "dependencies": {
    "@stomp/rx-stomp": "^0.3.4",
    "@stomp/stompjs": "^5.4.3",
    "@svgr/core": "^5.2.0",
    "@types/react": "^16.9.13",
    "@types/react-dom": "^16.9.4",
    "@types/sockjs-client": "^1.1.1",
    "ip": "^1.1.5",
    "office-ui-fabric-react": "^7.99.0",
    "react": "^16.12.0",
    "react-awesome-tabs": "^0.1.0",
    "react-dom": "^16.12.0",
    "react-inlinesvg": "^1.2.0",
    "react-router-dom": "^5.1.2",
    "react-sortablejs": "^2.0.11",
    "react-svgmt": "^1.1.11",
    "rxjs": "^6.0.0",
    "soap": "^0.31.0",
    "socket.io": "^2.3.0",
    "socket.io-json-parser": "^2.1.0",
    "sockjs-client": "^1.4.0",
    "text-encoding": "^0.7.0"
  },
  "devDependencies": {
    "@types/ip": "^1.1.0",
    "@types/mocha": "^5.2.7",
    "@types/react-router": "^4.4.5",
    "@types/react-router-dom": "^4.3.5",
    "@types/socket.io-client": "^1.4.32",
    "acorn": "^6.4.1",
    "ajv": "^6.10.2",
    "copy-pkg-json-webpack-plugin": "0.0.38",
    "cross-env": "^5.2.1",
    "css-loader": "^1.0.1",
    "electron": "^3.1.13",
    "electron-builder": "^20.44.4",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.17.0",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "lodash": "^4.17.15",
    "mocha": "^5.2.0",
    "rimraf": "^2.7.1",
    "source-map-loader": "^0.2.4",
    "spectron": "^5.0.0",
    "style-loader": "^0.23.1",
    "ts-loader": "^5.4.5",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.20.1",
    "tslint-microsoft-contrib": "^5.2.1",
    "typescript": "^3.7.2",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/splitfirex/Electron-windowManager.git"
  },
  "keywords": [
    "boilerplate",
    "Electron",
    "React",
    "Typescript",
    "Webpack"
  ]
}
